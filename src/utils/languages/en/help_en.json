  "massban": [
    "Bans multiple users from the server.",
    "\n**__Command:__** {p}massban [ `arguments` ]",
    "\n**__Arguments:__**",
    "`--channel` channel in which messages are searched",
    "`--reason` reason for the bans (required)",
    "`--created` accounts that are younger than 'x' minutes",
    "`--joined` accounts that joined `x` minutes ago",
    "`--no-avatar` user without avatar (no argument)",
    "`--no-roles` user without role (no argument)",
    "`--joined-before` users that are joined before the given userid",
    "`--joined-after` users who joined after the given userid",
    "`--regex` Regex of the usernames",
    "\n**All other arguments require --channel**",
    "`--contains` what appears in the message",
    "`--starts`: how the message starts",
    "`--ends`: how the message ends",
    "`--search`: how many messages should be searched (default 100; max 2000)",
    "`--after`: messages after the given messageid",
    "`--before`: messages before the given messageid",
    "`--files`: whether the message has a file (no argument)",
    "`--embeds`: whether the message has embeds (no argument)"
  ],
